{
    "name"          : "ng-boletas",
    "version"       : "1.0.0",
    "description"   : "Angular 2 web client for the cursame boletas service.",
    "main"          : "index.js",
    "private"       : true,
    "repository"    : {
        "type"      : "git",
        "url"       : "git+https://github.com/cursame/ng-boletas"
    },
    "scripts"       : {
        "app"           : "node startup.js",
        "start"         : "concurrently \"npm run tsc:w\" \"npm run lite\" \"npm run app\" ",
        "lite"          : "lite-server",
        "tsc"           : "tsc",
        "tsc:w"         : "tsc -w",
        "postinstall"   : "typings install",
        "typings"       : "typings"
    },
    "bugs"          : {
        "url"       : "https://github.com/cursame/ng-boletas/issues"
    },
    "dependencies"  : {
